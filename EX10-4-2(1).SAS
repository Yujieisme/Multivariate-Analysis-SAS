/*  EXAMPLES 10.4.2, 10.5.1, and 10.5.2  */
DATA CHEM;
  INFILE 'T10_1_CHEM.dat';
  INPUT NUM Y1 Y2 Y3 X1 X2 X3;
  X1X2 = X1*X2;   X1SQ = X1*X1;
  X1X3 = X1*X3;   X2SQ = X2*X2;
  X2X3 = X2*X3;   X3SQ = X3*X3;
RUN;
PROC REG;
  MODEL Y1 Y2 Y3 = X1 X2 X3;
  OVERALL:  MTEST /PRINT CANPRINT;
  TITLE 'EXAMPLES 10.4.2, 10.5.1, and 10.5.2';
RUN;
PROC REG;
  MODEL Y1 Y2 Y3 = X1 X2 X3 X1X2 X1X3 X2X3 X1SQ X2SQ X3SQ;
  OVERALL9:  MTEST /PRINT MSTAT=EXACT;
  Partial6:   MTEST X1X2,X1X3,X2X3,X1SQ,X2SQ,X3SQ/PRINT MSTAT=EXACT;
  Partial3:   MTEST X1X2,X1X3,X2X3/PRINT MSTAT=EXACT;
RUN;
